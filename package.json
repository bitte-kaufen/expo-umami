{
  "name": "@bitte-kaufen/expo-umami",
  "version": "0.1.0",
  "description": "Umami analytics integration for Expo apps with batching and offline support",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "clean": "rm -rf build",
    "prepare": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "expo",
    "umami",
    "analytics",
    "tracking",
    "react-native",
    "bitte-kaufen"
  ],
  "author": "Sascha Depold <sascha@depold.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bitte-kaufen/expo-umami.git"
  },
  "peerDependencies": {
    "expo": "*",
    "react": "*",
    "react-native": "*"
  },
  "dependencies": {
    "@expo/config-plugins": "^8.0.0",
    "@react-native-async-storage/async-storage": "^1.23.0",
    "change-case": "^5.4.4",
    "expo-application": "*",
    "expo-constants": "*",
    "expo-device": "*",
    "expo-localization": "^17.0.8"
  },
  "devDependencies": {
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/react": "^18.2.0",
    "@types/react-native": "^0.72.0",
    "@vitest/coverage-v8": "^4.0.15",
    "expo": "^51.0.0",
    "react": "^18.2.0",
    "react-native": "^0.74.0",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.15"
  }
}
